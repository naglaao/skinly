#:kivy 1.11.1

#: import Factory kivy.factory.Factory


<CreateAccountWindow>:
    name: "create"

    namee: namee
    email: email
    password: passw

    FloatLayout:
        cols:1

        FloatLayout:
            size: root.width, root.height/2

            canvas:
                Color:
                    rgb:  1, 1,1,1
                Rectangle:
                    pos: self.pos
                    size: self.size

            Label:
                text: "Create an Account"
                bold: True
                color:(199/255, 33/255, 106/255,.9)
                size_hint: 0.8, 0.2
                pos_hint: {"x":0.1, "top":1}
                font_size: (root.width**2 + root.height**2) / 13**4
                
            
            TextInput:
                id: namee
                pos_hint: {"x":0.27, "top":0.76}
                size_hint: 0.60, 0.06
                multiline: False
                font_size: (root.width**2 + root.height**2) / 13**4
                hint_text: "Name"
                markup: True

            Image:
                source:'imag/username.png'
                size_hint: .10, .13
                pos_hint: {"x": 0.15, "top": 0.8}


            TextInput:
                id: email
                pos_hint: {"x":0.27, "top":0.8-0.17}
                size_hint: 0.60, 0.06
                multiline: False
                font_size: (root.width**2 + root.height**2) / 13**4
                hint_text: "Email"
                markup: True

            Image:
                :'pictures/email.png'
                size_hint: .10, .13
                pos_hint: {"x": 0.15, "top": 0.8-0.13}

            TextInput:
                id: passw
                pos_hint: {"x":0.27, "top":0.8-0.15*2}
                size_hint: 0.60, 0.06
                multiline: False
                password: True
                font_size: (root.width**2 + root.height**2) / 13**4
                hint_text: "Password"
                markup: True
            
            Image:
                source:'imag/password.png'
                size_hint: .10, .13
                pos_hint: {"x": 0.15, "top":0.8-0.13*2}

        LabelButton:
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.6, 0.1
            font_size: (root.width**2 + root.height**2) / 14**4
            text: "[u]Already have an account? Log In[/u]"
            color:(106/255, 156/255, 237/255)
            markup: True
            on_release:
                root.login()
                root.manager.transition.direction = "left"
                

        Button:
            pos_hint:{"x":0.20,"y":0.25}
            size_hint: 0.66, .06
            background_color: 199/255, 33/255, 106/255,.9
            text: "SUBMIT"
            bold: True
        
            on_release:
                root.submit()
                root.manager.transition.direction = "left"
            
    
         
<LoginWindow>:
    name: "login"

    email: email
    password: password

    FloatLayout:

        canvas:
            Color:
                rgb:  1, 1,1,1
            Rectangle:
                pos: self.pos
                size: self.size
 
        Image:
            source:'imag/users icon4.png'
            size_hint: .5, .5
            pos_hint: {"x": .25, "top": 1.05}


        TextInput:
            id: email
            font_size: (root.width**2 + root.height**2) / 13**4
            multiline: False
            pos_hint: {"x": 0.27 , "top":0.60}
            size_hint: 0.60, 0.06
            hint_text: "Email"
            color:1, 1,1,1
            markup: True
        
 
        Image:
            source:'imag/email.png'
            size_hint: .10, .13
            pos_hint: {"x": 0.15, "top": 0.64}
       

        TextInput:
            id: password
            font_size: (root.width**2 + root.height**2) / 13**4
            multiline: False
            pos_hint: {"x": 0.27, "top":0.50}
            size_hint: 0.60, 0.06
            hint_text: "Password"
            color:1, 1,1,1
            password: True
            markup: True
        Image:
            source:'imag/password.png'
            size_hint: .10, .13
            pos_hint: {"x": 0.15, "top": 0.54}

        Button:
            pos_hint:{"x":0.20,"y":0.25}
            size_hint: 0.66, .06
            background_color: 199/255, 33/255, 106/255,.9
            text: "LOGIN"
            bold: True
            
            on_release:
                root.manager.transition.direction = "up"
                root.loginBtn()
           
        LabelButton:
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.6, 0.1
            font_size: (root.width**2 + root.height**2) / 14**4
            text: "[u]Don't have an account? Create One[/u]"
            color:(106/255, 156/255, 237/255)
            markup: True
            on_release:
                root.manager.transition.direction = "right"
                root.createBtn()

